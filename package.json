  "homepage": "https://github.com/sametadsan/vue-html-to-pdf-lenora",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sametadsan/vue-html-to-pdf-lenora.git"
  },
  "bugs": {
    "url": "https://github.com/sametadsan/vue-html-to-pdf-lenora/issues"
  },
  "devDependencies": {
    "@vue/cli-service": "^5.0.8",
    "sass": "^1.85.1",
    "sass-loader": "^16.0.5",
    "vite": "^6.2.1"
  }
}
