    "build-lib": "vue-cli-service build --target lib --name vue-html-to-pdf-lenora src/index.js",
    "dev": "vue-cli-service serve"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue3-html2pdf": "^1.1.2"
